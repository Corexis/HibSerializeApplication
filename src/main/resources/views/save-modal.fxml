<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="130.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal"
            fx:controller="hibSerializerApp.view.SaveModalViewController">
    <children>
        <Button layoutX="286.0" layoutY="98.0" minWidth="100.0" mnemonicParsing="false" onAction="#save" text="Save"/>
        <Button layoutX="150.0" layoutY="98.0" minWidth="100.0" mnemonicParsing="false" onAction="#dontSave"
                text="Don't save"/>
        <Button layoutX="14.0" layoutY="98.0" minWidth="100.0" mnemonicParsing="false" onAction="#cancel"
                text="Cancel"/>
        <Text layoutX="16.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Do you want to save the changes?">
            <font>
                <Font name="Arial" size="24.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
